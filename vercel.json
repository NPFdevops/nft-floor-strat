{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_RAPIDAPI_KEY": "@vite_rapidapi_key",
    "VITE_RAPIDAPI_HOST": "@vite_rapidapi_host",
    "VITE_POSTHOG_KEY": "@vite_posthog_key",
    "VITE_POSTHOG_HOST": "@vite_posthog_host"
  },
  "build": {
    "env": {
      "VITE_RAPIDAPI_KEY": "@vite_rapidapi_key",
      "VITE_RAPIDAPI_HOST": "@vite_rapidapi_host",
      "VITE_POSTHOG_KEY": "@vite_posthog_key",
      "VITE_POSTHOG_HOST": "@vite_posthog_host"
    }
  },
  "functions": {
    "api/strategies.js": {
      "maxDuration": 30
    },
    "api/holdings.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/strategies",
      "destination": "/api/strategies"
    },
    {
      "source": "/api/holdings(.*)",
      "destination": "/api/holdings$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
