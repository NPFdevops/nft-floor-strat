{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/strategies.js": {
      "maxDuration": 30
    },
    "api/holdings.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/strategies",
      "destination": "/api/strategies"
    },
    {
      "source": "/api/holdings(.*)",
      "destination": "/api/holdings$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
